started=False
command =''
while True:
    command=input('>')
    if command =='start':
        if started:
            print('car is already start!')
        else:
            started=True
            print('car is started')   
    elif command == 'stop':
        if not started: 
            print('car is already stpped')
        else :
                started=False
                print('car is stop')             
    elif command=='quit':
        break
    else:
        print('do not undrestand')   
